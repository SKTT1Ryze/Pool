life Animal {
  let name: string;
}

// 接口声明
interface Swim {
  fn swim() -> void;
}

// 鱼声明类型的声明
life Fish inherit Animal implements Swim {
  let size: usize;
  fn swim() -> void {
    println("swimming!");
  }
}

// 虾声明类型的声明
life Shrimp inherit Animal {
  let lenght: f32;
}

fn main() -> void {
  let fish: Fish;
  let a: usize;
  // let b: f32;
  let b: usize;
  // let c: f32;
  let c: usize;
  let i: usize;
  fish = spawn(Fish);
  fish.swim();
  c = a + b;
  for (i = 0; i < 10; i= i + 1) {
    println("for loop");
  }
  while(true) {}
  return;
}